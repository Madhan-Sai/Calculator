<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hp.calculator.Main"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp"
    tools:ignore="GradleDependency">

    <Button
        android:id="@+id/sqr"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="sqrt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/power"
        app:layout_constraintTop_toBottomOf="@+id/button0"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/power"
        android:layout_width="88dp"
        android:layout_height="0dp"
        android:text="pow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/sqr"
        app:layout_constraintRight_toLeftOf="@+id/clear"
        app:layout_constraintTop_toBottomOf="@+id/button0"
        app:layout_constraintVertical_bias="0.469"
        tools:ignore="HardcodedText"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="18dp" />

    <Button
        android:id="@+id/clear"
        android:layout_width="88dp"
        android:layout_height="0dp"
        android:text="clr"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/power"
        app:layout_constraintRight_toLeftOf="@+id/equal"
        app:layout_constraintTop_toBottomOf="@+id/button0"
        app:layout_constraintVertical_bias="0.522"
        tools:ignore="HardcodedText"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="16dp" />

    <Button
        android:id="@+id/equal"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="="
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/clear"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/div"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/sub"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="-"
        app:layout_constraintBottom_toTopOf="@+id/mul"
        app:layout_constraintLeft_toRightOf="@+id/button6"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/mul"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="*"
        app:layout_constraintBottom_toTopOf="@+id/div"
        app:layout_constraintLeft_toRightOf="@+id/button9"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sub"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/div"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:text="/"
        app:layout_constraintBottom_toTopOf="@+id/equal"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mul"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button1"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="1"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/button2"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button2"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="2"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintLeft_toRightOf="@+id/button1"
        app:layout_constraintRight_toLeftOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button3"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="3"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintLeft_toRightOf="@+id/button2"
        app:layout_constraintRight_toLeftOf="@+id/add"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button4"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="4"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/button1"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button5"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="5"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintLeft_toRightOf="@+id/button4"
        app:layout_constraintRight_toLeftOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button6"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="6"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintLeft_toRightOf="@+id/button5"
        app:layout_constraintRight_toLeftOf="@+id/sub"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button7"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="7"
        app:layout_constraintBottom_toTopOf="@+id/button0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/button8"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button8"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="8"
        app:layout_constraintBottom_toTopOf="@+id/button0"
        app:layout_constraintLeft_toRightOf="@+id/button7"
        app:layout_constraintRight_toLeftOf="@+id/button9"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button9"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="9"
        app:layout_constraintBottom_toTopOf="@+id/button0"
        app:layout_constraintLeft_toRightOf="@+id/button8"
        app:layout_constraintRight_toLeftOf="@+id/mul"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button0"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/sqr"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/div"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        tools:ignore="HardcodedText"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="42dp"
        android:layout_marginEnd="41dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginLeft="42dp"
        android:layout_marginRight="41dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="315dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/add"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/button1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        tools:ignore="MissingConstraints"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/add"
        android:layout_width="88dp"
        android:layout_height="70dp"
        android:text="+"
        app:layout_constraintBottom_toTopOf="@+id/sub"
        app:layout_constraintLeft_toRightOf="@+id/button3"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        tools:ignore="HardcodedText" />

</android.support.constraint.ConstraintLayout>
